    Pour l'attaque
    	public void attackCritters(){
    		for(Tower t: towerList){
    			if(t.getTargetCritter()!= null &&t.canAttack()){
    				attackCritter(t);
    				t.setTimeOfLastAttack(System.currentTimeMillis());
    			}
    		}
    	}
